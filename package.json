	"scripts": {
		"serve": "npm run serve:dev",
		"serve:dev": "nodemon --transpile-only -P tsconfig.json server/api/app.ts",
		"serve:prod": "nodemon build/server/api/app.js",
		"build": "rm -rf build && npm run build:ui && npm run build:server",
		"build:server": "rm -rf build/server build/knexfile.* && tsc",
		"build:ui": "rm -rf build && mkdir build\\ui && cd ui && npm run build && cd .. && mv ui/build/* build/ui && rm -rf ui/build",
		"seed": "npx knex seed:run",
		"migrate": "npx knex migrate:latest --knexfile knexfile.ts",
		"migrate:fresh": "npx knex migrate:down --knexfile knexfile.ts && npm run migrate"
	}
}
